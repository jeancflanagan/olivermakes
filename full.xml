---
title: olivermak.es
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ page.title }}</title>
  <link href="{{ site.link.subscribe.full }}" rel="self"/>
  <link href="{{ site.url }}/"/>
  <icon>{{ site.icons-url }}/favicon64.png</icon>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.url }}/</id>
  <author>
    <name>{{ site.author.name }}</name>
  </author>
  <rights>CC BY-NC {{ site.author.name }}</rights>
  
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title | strip_html | nbsp_escape }}</title>
    <link href="{{ site.url }}{{ post.url }}"/>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.updated | date_to_xmlschema }}</updated>
    <id>tag:{{ site.domain }},{{ post.unique-id }}</id>
    <content type="html">
      {{ post.content | syndication_escape | xml_escape }}
    </content>
  </entry>
  {% endfor %}
</feed>